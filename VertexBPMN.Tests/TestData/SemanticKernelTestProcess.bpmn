<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
				  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
				  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  id="Definitions_2"
                  targetNamespace="http://bpmn.io/schema/bpmn">
  <bpmn:process id="SemanticKernelTestProcess" name="Semantic Kernel Test Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="SK_ServiceTask_1" name="Semantic Kernel Service Task" implementation="semanticKernelServiceTask">
      <bpmn:extensionElements>
        <!-- SemanticKernelServiceTaskHandler spezifische Attribute -->
        <bpmn:property name="provider" value="OpenAI"/>
        <bpmn:property name="modelId" value="gpt-4o"/>
        <bpmn:property name="prompt" value="customerMessage"/>
        <bpmn:property name="resultVariable" value="llmResult"/>
        <!-- Optional: endpoint, falls benötigt -->
        <!-- <bpmn:property name="endpoint" value="https://api.openai.com/v1/chat/completions"/> -->
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_2</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="SK_ServiceTask_1"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="SK_ServiceTask_1" targetRef="EndEvent_1"/>
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_2">
    <bpmndi:BPMNPlane id="BPMNPlane_2" bpmnElement="SemanticKernelTestProcess">
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="100" y="100" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="SK_ServiceTask_1_di" bpmnElement="SK_ServiceTask_1">
        <dc:Bounds x="200" y="90" width="100" height="60"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_1">
        <dc:Bounds x="350" y="100" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="136" y="118"/>
        <di:waypoint x="200" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="300" y="120"/>
        <di:waypoint x="350" y="118"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>
